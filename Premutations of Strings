#include <stdio.h>
#include <stdlib.h>
#include <string.h>

/* swap two string pointers */
void swap(char **a, char **b) {
    char *tmp = *a;
    *a = *b;
    *b = tmp;
}

/* reverse part of array of strings from start to end (inclusive) */
void reverse(char **s, int start, int end) {
    while (start < end) {
        swap(&s[start], &s[end]);
        start++;
        end--;
    }
}

/* generate next lexicographical permutation of array s with length n
   returns 1 if next permutation exists, 0 if it was the last permutation */
int next_permutation(int n, char **s) {
    if (n <= 1) return 0;

    int i;
    for (i = n - 2; i >= 0; i--) {
        if (strcmp(s[i], s[i + 1]) < 0) break;
    }

    if (i < 0) return 0;

    int j;
    for (j = n - 1; j > i; j--) {
        if (strcmp(s[j], s[i]) > 0) break;
    }

    swap(&s[i], &s[j]);
    reverse(s, i + 1, n - 1);
    return 1;
}

/* comparator for qsort to sort pointers by the strings they point to */
int cmp_strptr(const void *pa, const void *pb) {
    const char *const *a = pa;
    const char *const *b = pb;
    return strcmp(*a, *b);
}
int main()
{
	char **s;
	int n;
	scanf("%d", &n);
	s = calloc(n, sizeof(char*));
	for (int i = 0; i < n; i++)
	{
		s[i] = calloc(11, sizeof(char));
		scanf("%s", s[i]);
	}
	do
	{
		for (int i = 0; i < n; i++)
			printf("%s%c", s[i], i == n - 1 ? '\n' : ' ');
	} while (next_permutation(n, s));
	for (int i = 0; i < n; i++)
		free(s[i]);
	free(s);
	return 0;
}
